{
   "swagger":"2.0",
   "info":{
      "description":"This microservice provides APIs for Frontend Events Application.",
      "version":"1.0",
      "title":"Frontend Events Service API",
      "service":"enrollmentservice-2.0"
   },
   "host":"frontend-events.us-west-2.staging.us.experiancs.internal",
   "basePath":"/",
   "tags":[
      {
         "name":"Public routes",
         "description":"Frontend Events Controller"
      },
      {
         "name":"internal-frontend-events-controller",
         "description":"Internal Frontend Events Controller"
      }
   ],
   "paths":{
      "/api/frontend-events/events":{
         "get":{
            "tags":[
               "Public routes"
            ],
            "summary":"Returns a text/event-stream for the required x-customerId and x-sessionId headers and required lastEventId query string parameter.",
            "operationId":"getEventStreamWithLastEventIdParamUsingGET",
            "produces":[
               "text/event-stream"
            ],
            "parameters":[
               {
                  "name":"lastEventId",
                  "in":"query",
                  "description":"lastEventId",
                  "required":true,
                  "type":"string"
               }
            ],
            "responses":{
               "200":{
                  "description":"OK",
                  "schema":{
                     "$ref":"#/definitions/Flux«ServerSentEvent«D2cEvent»»"
                  }
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               },
               "404":{
                  "description":"Not Found"
               }
            }
         }
      },
      "/api/frontend-events/events/test":{
         "post":{
            "tags":[
               "Public routes"
            ],
            "summary":"Sends a test event for the given x-customerId and x-sessionId headers.",
            "operationId":"sendTestEventUsingPOST",
            "consumes":[
               "application/json"
            ],
            "produces":[
               "*/*"
            ],
            "responses":{
               "200":{
                  "description":"OK"
               },
               "201":{
                  "description":"Created"
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               },
               "404":{
                  "description":"Not Found"
               }
            }
         }
      },
      "/internal/frontend-events/events":{
         "post":{
            "tags":[
               "internal-frontend-events-controller"
            ],
            "summary":"Publishes event to event stream.",
            "operationId":"publishToEventStreamUsingPOST",
            "consumes":[
               "application/json"
            ],
            "produces":[
               "*/*"
            ],
            "parameters":[
               {
                  "in":"body",
                  "name":"d2cEvent",
                  "description":"d2cEvent",
                  "required":true,
                  "schema":{
                     "$ref":"#/definitions/D2cEvent"
                  }
               }
            ],
            "responses":{
               "200":{
                  "description":"OK"
               },
               "201":{
                  "description":"Created"
               },
               "401":{
                  "description":"Unauthorized"
               },
               "403":{
                  "description":"Forbidden"
               },
               "404":{
                  "description":"Not Found"
               }
            }
         }
      }
   },
   "definitions":{
      "D2cEvent":{
         "type":"object",
         "properties":{
            "metadata":{
               "$ref":"#/definitions/Metadata"
            },
            "payload":{
               "type":"object"
            }
         },
         "title":"D2cEvent"
      },
      "Flux«ServerSentEvent«D2cEvent»»":{
         "type":"object",
         "properties":{
            "prefetch":{
               "type":"integer",
               "format":"int32"
            }
         },
         "title":"Flux«ServerSentEvent«D2cEvent»»"
      },
      "Metadata":{
         "type":"object",
         "properties":{
            "correlationId":{
               "type":"string"
            },
            "customerId":{
               "type":"string"
            },
            "eventId":{
               "type":"string"
            },
            "eventType":{
               "type":"string"
            },
            "schemaVersion":{
               "type":"string"
            },
            "serviceName":{
               "type":"string"
            },
            "sessionId":{
               "type":"string"
            },
            "timestamp":{
               "type":"string"
            }
         },
         "title":"Metadata"
      },
      "ServerSentEvent«D2cEvent»":{
         "type":"object",
         "title":"ServerSentEvent«D2cEvent»"
      }
   }
}
